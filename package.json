{
  "name": "astro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "cz",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,html}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sanitize.css": "^12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "bitbucket:magicpowered/astro"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --color --max-warnings 0 --no-ignore"
    ],
    "*.{js,jsx,ts,tsx,css,scss,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/react": "17.0.14",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "typescript": "4.3.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "commitizen": "^4.2.4",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "semantic-release": "^17.4.4"
  }
}
