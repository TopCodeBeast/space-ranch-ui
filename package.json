    "next-optimized-images": "^2.6.2",
    "next-plugin-svgr": "^1.0.0",
    "react": "17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-measure": "^2.5.2",
