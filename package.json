{
  "name": "astro",
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ]
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate-assets-index && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "cz",
