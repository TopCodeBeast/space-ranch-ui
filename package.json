  "repository": {
    "type": "git",
    "url": "bitbucket:magicpowered/astro"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --color --max-warnings 0 --no-ignore"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --quiet --syntax=scss --fix",
      "prettier --write"
    ]
  },
