    "react-popper-tooltip": "^4.3.0",
    "react-text-truncate": "^0.17.0",
    "react-textarea-autosize": "^8.3.3",
    "react-use": "^17.2.4",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7",
    "sanitize.css": "^12.0.1",
    "svg-sprite-loader": "^6.0.9",
